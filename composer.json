{
    "name": "oliverlumby/interactions",
    "authors": [
        {
            "name": "Oliver Lumby",
            "email": "me@oliverlumby.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/console": "5.5.*|5.6.*",
        "illuminate/support": "5.5.*|5.6.*"
    },
    "require-dev": {
        "orchestra/testbench": "~3.5|~3.6",
        "phpunit/phpunit": "~6.0|~7.0"
    },
    "autoload": {
        "psr-4": {
            "Lumby\\Interactions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lumby\\Interactions\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lumby\\Interactions\\InteractionServiceProvider"
            ]
        }
    }
}
